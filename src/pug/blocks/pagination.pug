.pagination
	ul.pagination__list
		li.arrow.arrow--prev.disabled
			a(href="#")
				+icon("breadcrumbs_arrow","icon--size_mod")
		li.active
			a(href="#") 1
		li
			a(href="#") 2
		li
			a(href="#") 3
		li
			a(href="#") 4
		li.dots
			span ...
		li
			a(href="#") 8
		li.arrow.arrow--next
			a(href="#")
				+icon("breadcrumbs_arrow","icon--size_mod")
